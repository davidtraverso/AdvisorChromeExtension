{
"name": "Advisor Extension Assistant",
"version": "1.0",
"description": "Try to make pricing easier to read and phrasing easier to remember.",
 "permissions": ["declarativeContent", "storage", "activeTab", "tabs"],
 "icons": {
    "128": "icon_128.png",
    "96": "icon_96.png",
    "48": "icon_48.png",
    "32": "icon_32.png",
    "16": "icon_16.png"
 },
"content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }
],
"manifest_version": 2
}